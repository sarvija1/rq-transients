{
   "compilerOptions": {
     "noEmit": true,
     "target": "esnext",
     "module": "nodenext",
     "rewriteRelativeImportExtensions": true,
     "erasableSyntaxOnly": true,
     "verbatimModuleSyntax": true,
     "moduleResolution": "node",
     "allowImportingTsExtensions": true,
     "strict": true,
     "noImplicitAny": true
  }
}
